@model IEnumerable<GettingStarted.Models.Parent>


@{
    ViewBag.Title = "Parents";
}

<h2>Parents</h2>

<p>
    <a asp-controller="Parent" asp-action="Create">Create New</a>
</p>

<table class="table">
    <tr>
        <th>Name</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)            
            </td>
        </tr>
    }
</table>